#. module: pawnshop
#: report:pawn_jor6_report:0
msgid "select shop.name shop_name, shop.reg_book, shop.reg_number, shop.full_address shop_address, 	pw.date_order, pw.name pawn_number, rp.name cust_name, 	rp.address_full cust_address, pwl.name item_desc, pwl.product_qty qty, 	uom.name uom_name, coalesce(pwl.carat,0) as carat, 	coalesce(pwl.gram,0) as gram, 	plp.other_property, 	floor(pp.price_pawned) baht, floor(pp.price_pawned::numeric % 1 * 100) satang, 	floor(pw.amount_pawned) total_baht, floor(pw.amount_pawned::int % 1*100) total_satang from pawn_order pw join pawn_shop shop on shop.id = pw.pawn_shop_id 	join pawn_order_line pwl on pwl.order_id = pw.id 	join product_product pp on pp.order_line_id = pwl.id 	join product_uom uom on uom.id = pwl.product_uom 	join res_partner rp on rp.id = pw.partner_id 	left outer join pawn_order_line_properties plp on plp.order_line_id = pwl.id where date_jor6 = $P{from_date} 	and pawn_shop_id = $P{pawn_shop_id} 	and journal_id = $P{journal_id} 	and rule_id = $P{pawn_rule_id} order by date_expired, pawn_number, pwl.id"
msgstr "select shop.name shop_name, shop.reg_book, shop.reg_number, shop.full_address shop_address, 	pw.date_order, pw.name pawn_number, rp.name cust_name, 	rp.address_full cust_address, pwl.name item_desc, pwl.product_qty qty, 	uom.name uom_name, coalesce(pwl.carat,0) as carat, 	coalesce(pwl.gram,0) as gram, 	plp.other_property, 	floor(pp.price_pawned) baht, floor(pp.price_pawned::numeric % 1 * 100) satang, 	floor(pw.amount_pawned) total_baht, floor(pw.amount_pawned::int % 1*100) total_satang from pawn_order pw join pawn_shop shop on shop.id = pw.pawn_shop_id 	join pawn_order_line pwl on pwl.order_id = pw.id 	join product_product pp on pp.order_line_id = pwl.id 	join product_uom uom on uom.id = pwl.product_uom 	join res_partner rp on rp.id = pw.partner_id 	left outer join pawn_order_line_properties plp on plp.order_line_id = pwl.id where date_jor6 = $P{from_date} 	and pawn_shop_id = $P{pawn_shop_id} 	and journal_id = $P{journal_id} 	and rule_id = $P{pawn_rule_id} order by date_expired, pawn_number, pwl.id"

